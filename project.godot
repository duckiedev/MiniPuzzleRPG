; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "",
"class": "GitAPI",
"language": "NativeScript",
"path": "res://addons/godot-git-plugin/git_api.gdns"
} ]
_global_script_class_icons={
"GitAPI": ""
}

[application]

config/name="MiniPuzzleRPG"
run/main_scene="res://Main/Main.tscn"
config/icon="res://GFX/player_single.png"

[autoload]

Data="*res://Main/Data/Data.tscn"
SceneChanger="*res://Main/SceneChanger.tscn"
SoundFXPlayer="*res://Main/Audio/SoundFXPlayer.tscn"
MusicPlayer="*res://Main/Audio/MusicPlayer.tscn"
AudioManager="*res://Main/Audio/AudioManager.tscn"
Player="*res://Main/Player/Player.tscn"

[display]

window/size/width=160
window/size/height=144
window/size/test_width=480
window/size/test_height=432
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[gdnative]

singletons=[ "res://addons/godot-git-plugin/git_api.gdnlib" ]

[gui]

theme/custom_font="res://GFX/UI/font.tres"

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
